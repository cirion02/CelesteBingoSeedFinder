	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch board gen",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\dev\\GenerateBoardsFile.js"
		},

		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Launch gen to json",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}\\dev\\BingoGenToJson.js"
		},
		
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "externalTerminal"
		}
	]
}